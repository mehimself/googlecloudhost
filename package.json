    "clean": "rimraf dist",
    "clean:dll": "rimraf dll",
    "compile": "npm run lint && cross-env env.DEBUG=app:* node ./server/bin/compile",
    "deploy": "gcloud ./server/bin/www deploy",
    "system-test": "repo-tools test ./server/bin/www",
    "test": "npm run system-test",
    "e2e-test": "repo-tools test deploy",
    "lint": "cross-env env.NODE_ENV=production eslint --ext .js,.vue src",
